{
  "name": "contract-source-viewer",
  "displayName": "Contract Source Viewer",
  "description": "Fetch and view smart contract source code from blockchain explorers with syntax highlighting",
  "version": "1.0.2",
  "publisher": "junaire",
  "author": {
    "name": "Jun Zhang",
    "url": "https://github.com/junaire"
  },
  "license": "MIT",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Extension Packs"
  ],
  "keywords": [
    "blockchain",
    "smart contract",
    "solidity",
    "ethereum",
    "bsc",
    "polygon",
    "source code",
    "contract viewer",
    "web3",
    "defi"
  ],
  "homepage": "https://github.com/junaire/contract-source-viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/junaire/contract-source-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/junaire/contract-source-viewer/issues"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "contractSourceViewer.fetchSource",
        "title": "Fetch Contract Source Code",
        "category": "Contract Source Viewer"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "contractSourceViewer.fetchSource"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.12.2"
  }
}
